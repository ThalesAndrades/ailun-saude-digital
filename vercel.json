{
  "version": 2,
  "name": "ailun-saude",
  "alias": ["ailun-saude.vercel.app"],
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 30
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "env": {
    "RAPIDOC_BASE_URL": "@rapidoc_base_url",
    "RAPIDOC_CLIENT_ID": "@rapidoc_client_id",
    "RAPIDOC_TOKEN": "@rapidoc_token",
    "COOKIE_SECURE": "true",
    "NODE_ENV": "production"
  },
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}